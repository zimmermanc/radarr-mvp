# Test Configuration for HDBits Analysis
# This file contains minimal configuration for testing

# Database
DATABASE_URL=postgresql://radarr:radarr@localhost:5432/radarr_test

# HDBits (you need to fill in your actual credentials)
HDBITS_USERNAME=your_username_here
HDBITS_PASSKEY=your_passkey_here
HDBITS_SESSION_COOKIE=

# Analysis Settings (quick test values)
HDBITS_ANALYSIS_MAX_PAGES=5
HDBITS_ANALYSIS_RUN_MINUTES=2
HDBITS_ANALYSIS_BREAK_MINUTES=1
HDBITS_ANALYSIS_MAX_SEGMENTS=2
HDBITS_RATE_LIMIT_SECONDS=3

# TMDB (existing from .env)
TMDB_API_KEY=YOUR_TMDB_API_KEY_HERE

# Logging
RUST_LOG=info
LOG_FILE=/tmp/radarr_test.log

# Feature flags for testing
DEBUG_MODE=true
USE_TEST_DATA=true